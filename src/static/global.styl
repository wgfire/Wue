
$font-size = 14px
$border-radius = 4px;
//$color-main=#2b85e4
* {
  margin: 0px;
  padding: 0px;
  box-size: border-box;
}

body {
  font-size: $font-size;
  background: $color-main;
}
li {
  list-style none
}
ul {
  margin: 0px;
  padding: 0px
  margin-block-start: 0em;
  margin-block-end: 0em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  padding-inline-start: 0px;
}